{
  "manifest_version" : 2,

  "name" : "GoogleBookmarkHelper",
  "description" : "Google Bookmark に登録する",
  "version" : "1.0.1",

  "permissions" : [
    "tabs",
    "activeTab",
    "https://*/",
    "http://*/"
  ],

  "background" : {
    "scripts" : ["background.js"]
  },

  "browser_action" : {
    "100" : "icon_blue.png",
    "101" : "icon_red.png",
    "102" : "icon.png",
    "default_icon" : "icon.png"
  }
}
