{
  "manifest_version" : 2,

  "name" : "GoogleBookmarkHelper",
  "description" : "Google Bookmark に登録する",
  "version" : "1.0",

  "permissions" : [
    "tabs",
    "activeTab",
    "https://*/",
    "http://*/"
  ],

  "background" : {
    "scripts" : ["background.js"]
  },

  "browser_action" : {
    "default_icon" : "icon.png"
  }
}
